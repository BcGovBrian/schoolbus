@{
    ViewBag.Title = "Error Occurred.";
}

<!DOCTYPE html>
<html>
<head>
    <style type="text/css">
        * {
            box-sizing: border-box;
        }

        div {
            display: block;
        }

        .outer-div {
            padding: 30px;
        }

        .inner-div {
            margin: 12% auto;
            width: 500px;
        }

        body {
            font-family: Myriad-Pro,Calibri,Arial,'sans serif';
            margin: 0px;
        }

        h4{
            color: #777;
        }

        #header {
            width: 100%;
            background-color: #036;
            height: 64px;
            border-bottom: 2px solid #fcba19;
            padding: 0 15px;
            margin: 0 auto;
        }

        img {
            vertical-align: middle;
            border: 0;
        }

        #logo {
            float: left;
        }

        #banner {
            float: left;
            margin-top: 20px;
            margin-left: 35px;
            margin-bottom: 10px;
            color: white;
            font-weight: 600;
            font-size: 28px;
        }

        #reloadButton {
            background-color: #0094ff;
            border: 4px;
            color: white;
            padding: 5px 20px;
            text-decoration: none;
            display: inline-block;
            font-size: 15px;
            margin: 4px 2px;
            cursor: pointer;
            border-radius: 8px;
        }

        #closeButton {
            background-color: white;
            border: 1px solid #777;
            color: black;
            padding: 5px 20px;
            text-decoration: none;
            display: inline-block;
            font-size: 15px;
            margin: 4px 2px;
            cursor: pointer;
            border-radius: 8px;
        }

        @@media only screen and (max-width: 730px){
          #header{
            height: 120px;
          }
        }
        
        @@media only screen and (max-width: 570px){
          #header{
            height: 140px;
          }
        }
        
        @@media only screen and (max-width: 380px){
          #header{
            height: 180px;
          }
        }
    </style>
    <script>
        function reloadPage() {
            window.location.reload();
        }

        function closePage() {
            window.close();
        }
    </script>
</head>
<body>
    <div id="header">
        <div id="logo">
            <img title="Government of B.C." alt="Government of B.C." src="data:image/png;base64,">
        </div>
        <h1 id="banner">CVSE School Bus Inspection Tracking System</h1>
    </div>
    <div class="outer-div">
        <div class="inner-div">
            @RenderBody()
        </div>
    </div>
</body>
</html>